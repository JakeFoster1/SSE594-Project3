<Window x:Class="LibraryDeskApp.Results"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryDeskApp"
        mc:Ignorable="d"
        Title="Results" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="Search Results" FontSize="30" HorizontalAlignment="Center"></TextBlock>
        <Grid>
       
        <DataGrid Name="dg_results" ItemsSource="{Binding Result}" ></DataGrid>
    </Grid>
    </StackPanel>
</Window>
